{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": [
      {
        "include": "app/content-templates/usecases/compile-template/templates/basic.handlebars",
        "outDir": "dist/src/app/content-templates/usecases/compile-template/templates/basic.handlebars"
      },
      {
        "include": ".env",
        "outDir": "dist/src"
      },
      {
        "include": ".env.development",
        "outDir": "dist/src"
      },
      {
        "include": ".env.test",
        "outDir": "dist/src"
      },
      {
        "include": ".env.production",
        "outDir": "dist/src"
      }
    ],
    "plugins": [
      "@nestjs/swagger"
    ]
  }
}
